# Optional: Analytics
VITE_GOOGLE_ANALYTICS_ID=your_ga_id

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_AI_SERVICE_URL=http://localhost:8080

# Security
CSRF_SECRET=your-csrf-secret-key-here 